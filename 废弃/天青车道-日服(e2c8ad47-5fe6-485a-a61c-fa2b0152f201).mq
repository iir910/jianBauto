Function 移动(x小,x大,y小,y大)
    TracePrint "{移动}"
    索敌结果=1
    Dim 敌坐标,偏移x,偏移y,dx,dy
    For i = 0 To k - 1
        If sc(p_map, 0.87, 300,"") = false Then 
            TracePrint "已经不在map,停止{移动}"
            Exit Function	
        End If   '若不在MAP就不需要继续点敌舰，退出函数
        //TracePrint 索敌坐标(i)
        敌坐标 = Split(索敌坐标(i), ",")
        TracePrint "移动目标：",敌坐标(0),",",敌坐标(1)
        偏移x= Rnd_(x小,x大)
        偏移y= Rnd_(y小,y大)
        dx = CLng (敌坐标(0)) 
        dy = CLng (敌坐标(1))
        TracePrint "////实际点击的坐标: ",dx + 偏移x,",",dy + 偏移y
        Tap dx +偏移x, dy + 偏移y
        dim 图组={s_无法到达,s_空袭,p_遇到伏击,p_出击编队,p_获得道具,s_潜艇攻击 }
        //        Delay 50
        Dim 序号=scEX(图组,0.92,7000,"")
        If 序号 = 0 Then '黄条消息
            TracePrint "////无法到达目标,等待"
            倒计时(3,"无法到达",150,700)
        ElseIf 序号 = 1 Then '空袭
            TracePrint "////遭遇空袭"
            倒计时 2, "遭遇空袭", 150, 700
            Tap dx + Rnd_(x小,x大), dy + Rnd_(y小,y大)
            //            Delay 5000
            Exit Function 
        ElseIf 序号 = 2 Then '伏击
            If 处理伏击() Then 
                Tap dx + Rnd_(x小, x大), dy + Rnd_(y小, y大)
                倒计时 (3, "继续移动", 150, 700)
            Else 
                Exit Function
            End If
        ElseIf 序号 = 3 Then '出击编队
            出击编队
            Exit Function
        ElseIf 序号 = 4 Then  '获取道具
            获得道具
            Tap dx + Rnd_(x小,x大), dy + Rnd_(y小,y大)
            倒计时 (3, "继续移动", 150, 700)
            //            Exit Function
        ElseIf 序号 = 5 Then '潜艇攻击
            TracePrint "潜艇攻击中..."
            倒计时 3, "潜艇攻击中..", 150, 700
            //            Tap dx + Rnd_(x小,x大), dy + Rnd_(y小,y大)
            //            倒计时 (3, "继续移动", 150, 700)
        ElseIf 序号 = -1 Then '超时
            TracePrint "移动超时，重点击刚才的坐标"
            Tap dx + Rnd_(x小,x大), dy + Rnd_(y小,y大)
            倒计时 (3, "继续移动", 150, 700)
        End If
        //        Delay 2000
    Next
End Function
