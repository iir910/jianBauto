    dim N次战后=ReadUIConfig ("捡子弹前本轮战斗次数",3)      //捡子弹前本轮战斗次数
    Dim t0=TickCount()
    Do
    //    FindMultiColor 143,90,1230,608,"F3F3F3","-10|-28|4A8FDB,4|-14|2C95C6,29|-13|F0F0F0,30|-20|EDEDED,31|-20|EEEEEE",0,0.9,intX,intY
        dim 子弹 = 索敌all("F3F3F3","-10|-28|4A8FDB,4|-14|2C95C6,29|-13|F0F0F0,30|-20|EDEDED,31|-20|EEEEEE", 0.91,"搜子弹")
        If 子弹 Then 
            TracePrint "发现子弹"
            If  本轮战斗次数 < clng(N次战后) Then 
                TracePrint  "{捡子弹}本轮战斗次数不足",N次战后,"忽略"
                Exit Function 
            End If
            移动(0,30,40,60)
            Exit Function
        Else 
            Delay 200
        End If
    Loop While TickCount()-t0< 1000 '超时1000ms
    TracePrint "////////////////////////////{捡子弹}：无子弹"
End Function
