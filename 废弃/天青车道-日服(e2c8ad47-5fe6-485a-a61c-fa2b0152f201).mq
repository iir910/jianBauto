Function 处理伏击
    TracePrint "p_遇到伏击 "
    If ReadUIConfig("伏击", true) Then 
        TracePrint "点击 规避"
        tap1 1008, 404, 300
        Dim 数组={p_出击编队,p_map }
        Select Case scex(数组, 0.9, 1500,"")
        Case 0
            TracePrint "规避失败"
            处理伏击=false
            出击编队
        Case 1
            TracePrint "规避成功"
            倒计时 3, "规避成功", 150, 700
            处理伏击=true
        Case -1
        End Select
    Else 
        TracePrint "点击 迎击"
        tap1 721,405,1000 '迎击	
    End If
End Function
